{
  "effectSettings": {
    "maxEffectsPerClick": 9,    // Număr maxim de efecte diferite într-un click
    "minEffectsPerClick": 3,    // Număr minim
    "majorCooldownClicks": 15,  // Câte click-uri trebuie să treacă pt. efecte majore
    "filterCooldownClicks": 8,  // Cooldown pt. filtre de body
    "defaultCooldown": 0,       // Cooldown standard dacă nu e specificat altul
    "disruptiveEffects": [      // Efecte care nu ar trebui să apară prea des împreună
        "FakeError", "ExplosionWarning", "SimulateMouseInvert", "FLY_EVERYTHING",
        "ULTIMATE_GLITCH", "VisualJumpscare", "AuditoryJumpscare", "FakeLoadingBar",
        "BgImageFlicker", "InvertColors", "BlurPage"
     ],
    "popupEffects": ["FakeError", "ExplosionWarning"] // Efecte tip popup
  },

  "effects": {
    // --- Common Minor Chaos (Weight ~10-15) ---
    "SmallShake": { "weight": 15, "cooldown": 0, "params": {"intensity": [3, 8], "duration": [40, 120]} },
    "RandomSound": { "weight": 15, "cooldown": 0, "params": {"freq": [40, 1800], "dur": [0.02, 0.12], "type": ["sine", "triangle", "square"], "vol": [0.1, 0.4]} },
    "FlashMsgBox": { "weight": 12, "cooldown": 0 },
    "ChangeButtonColor": { "weight": 10, "cooldown": 0, "target": "doNotClickBtn", "property": "backgroundColor", "valuesKey": "randomHexColors" },
    "RandomizeButtonText": { "weight": 10, "cooldown": 0, "target": "doNotClickBtn", "property": "innerText", "valuesKey": "buttonTexts" }, // Cheia 'buttonTexts' va fi definita in data.json sau folosita generic
    "ChangeCursor": { "weight": 10, "cooldown": 0 },
    "TinyGlitchButton": { "weight": 10, "cooldown": 0, "target": "doNotClickBtn", "params": {"intensity": [2, 4]} },
    "SingleEmoji": { "weight": 12, "cooldown": 0, "count": 1 },
    "RandomizeScoreColor": { "weight": 8, "cooldown": 0, "target": "scoreDisplay", "property": "color", "valuesKey": "randomHexColors" },
    "RandomizeTitleColor": { "weight": 8, "cooldown": 0, "target": "titleEl", "property": "color", "valuesKey": "randomHexColors" },
    "MultiSound": { "weight": 10, "cooldown": 0, "params": {"count": [2, 5]} },
    "ChangeCrazyText": { "weight": 10, "cooldown": 0 },
    "AddFakeStatus": { "weight": 12, "cooldown": 1 },
    "SpawnDynamicText": { "weight": 10, "cooldown": 1 }, // Necesita definirea functiei effect_SpawnDynamicText
    "LogBrowserGibberish": { "weight": 8, "cooldown": 0 },

    // --- Medium Brain Rot (Weight ~5-10) ---
    "EmojiRain": { "weight": 10, "cooldown": 0, "count": [10, 25] },
    "CrazyButton": { "weight": 10, "cooldown": 1 }, // Spawn buton aleatoriu
    "ChangeBgColor": { "weight": 8, "cooldown": 0 },
    "ScrambleTitle": { "weight": 7, "cooldown": 2, "target": "titleEl" }, // Necesita effect_ScrambleText
    "DistortTitle": { "weight": 7, "cooldown": 2, "target": "titleEl" }, // Necesita effect_DistortElement
    "ShiftLayoutCenter": { "weight": 6, "cooldown": 1, "target": "centerStage" }, // Necesita effect_ShiftElementLayout
    "WordSalad": { "weight": 7, "cooldown": 3 }, // Necesita effect_WordSalad
    "PlayMemeSound": { "weight": 9, "cooldown": 1 }, // Reda sunet din lista memeSounds
    "LoudSquareSound": { "weight": 6, "cooldown": 1, "params": {"freq": [30, 350], "dur": [0.3, 0.6], "vol": [0.6, 0.8], "type": ["square"]} },
    "GlitchTitle": { "weight": 6, "cooldown": 2, "target": "titleEl", "params": {"intensity": [5, 8]} },
    "DistortMessageBox": { "weight": 6, "cooldown": 2, "target": "messageBox" },
    "ShiftLayoutConsole": { "weight": 5, "cooldown": 1, "target": "consoleOutput" },
    "GlitchScore": { "weight": 5, "cooldown": 2, "target": "scoreDisplay", "params": {"intensity": [3, 6]} },
    "ChangePageTitle": { "weight": 7, "cooldown": 1 }, // Necesita effect_ChangePageTitle
    "ChangeAlienImage": { "weight": 6, "cooldown": 3 }, // Necesita effect_ChangeAlienImage si lista alienImages in data.json
    "ConsoleGibberish": { "weight": 5, "cooldown": 1 }, // Necesita effect_ConsoleGibberish
    "RandomizeScoreSize": { "weight": 5, "cooldown": 1, "target": "scoreDisplay", "property": "fontSize", "values": ["1rem", "1.5rem", "2.5rem", "3rem", "0.5rem", "4rem"]},
    "ChangeCSSVar": { "weight": 7, "cooldown": 0 }, // Necesita effect_ChangeCSSVar
    "RandomFontConsole": { "weight": 5, "cooldown": 1, "target": "consoleOutput" }, // Necesita effect_RandomFont
    "RandomFontMsgBox": { "weight": 5, "cooldown": 1, "target": "messageBox" },
    "HideAlien": { "weight": 4, "cooldown": 4, "target": "alienImage" },       // Necesita effect_ToggleElementVisibility
    "HideConsole": { "weight": 4, "cooldown": 4, "target": "consoleOutput" },
    "RandomBorderElement": { "weight": 6, "cooldown": 1, "targets": ["messageBox", "consoleOutput", "titleEl"] }, // Necesita effect_RandomBorderElement
    "RandomTextSpacing": { "weight": 5, "cooldown": 2, "targets": ["messageBox", "titleEl", "crazyTextEl"] }, // Necesita effect_RandomTextSpacing
    "ShowSpinner": { "weight": 4, "cooldown": 5 }, // Necesita effect_ShowSpinner

    // --- High Intensity / Rare (Weight ~3-5) ---
    "MultiCrazyButtons": { "weight": 5, "cooldown": 5, "count": [3, 7] },
    "FlyAlien": { "weight": 4, "cooldown": 6, "target": "alienImage" },       // Necesita effect_MakeElementFly
    "DisplayAscii": { "weight": 5, "cooldown": 3 }, // Necesita effect_DisplayAscii
    "BigScoreBonus": { "weight": 4, "cooldown": 8, "params": {"amount": [30, 70]} }, // Necesita effect_BigScoreBonus
    "ToggleButtonClick": { "weight": 5, "cooldown": 4 }, // Necesita effect_ToggleButtonClick
    "FlashRandomImage": { "weight": 5, "cooldown": 3 }, // Necesita effect_FlashRandomImage si lista flashImages in data.json
    "IntenseGlitchAlien": { "weight": 3, "cooldown": 6, "target": "alienImage", "params": {"intensity": [8, 12]} },
    "ScrambleMessageBox": { "weight": 4, "cooldown": 4, "target": "messageBox" },
    "ShiftLayoutAlien": { "weight": 3, "cooldown": 4, "target": "alienImage" },
    "FlyScore": { "weight": 3, "cooldown": 7, "target": "scoreDisplay" },
    "RandomFontTitle": { "weight": 4, "cooldown": 3, "target": "titleEl" },
    "RandomFontButton": { "weight": 4, "cooldown": 3, "target": "doNotClickBtn" },
    "HideScore": { "weight": 3, "cooldown": 6, "target": "scoreDisplay" },
    "ChangeFavicon": { "weight": 4, "cooldown": 5 }, // Necesita effect_ChangeFavicon si lista favicons in data.json

    // --- Filters (Cooldown set by filterCooldownClicks) ---
    "Pixelate": { "weight": 6, "cooldownKey": "filterCooldownClicks", "filterClass": "filter-pixelate" }, // Necesita effect_ToggleBodyFilter
    "BlurPage": { "weight": 5, "cooldownKey": "filterCooldownClicks", "filterClass": "filter-blur" },
    "HueRotate": { "weight": 5, "cooldownKey": "filterCooldownClicks", "filterClass": "filter-hue" },
    "ContrastBoost": { "weight": 4, "cooldownKey": "filterCooldownClicks", "filterClass": "filter-contrast" },
    "InvertColors": { "weight": 4, "cooldownKey": "filterCooldownClicks", "filterClass": "filter-invert" },
    "SepiaTone": { "weight": 3, "cooldownKey": "filterCooldownClicks", "filterClass": "filter-sepia" },

    // --- Maximum Brain Rot / Very Rare (Cooldown set by majorCooldownClicks) ---
    "AuditoryJumpscare": { "weight": 3, "cooldownKey": "majorCooldownClicks", "params": {"volume": [0.85, 1.25]} }, // Necesita effect_AuditoryJumpscare
    "VisualJumpscare": { "weight": 3, "cooldownKey": "majorCooldownClicks" }, // Necesita effect_VisualJumpscare si jumpscareImage in data.json
    "FakeError": { "weight": 4, "cooldownKey": "majorCooldownClicks" }, // Necesita effect_FakeError
    "ExplosionWarning": { "weight": 4, "cooldownKey": "majorCooldownClicks" }, // Necesita effect_ExplosionWarning
    "FakeLoadingBar": { "weight": 5, "cooldownKey": "majorCooldownClicks" }, // Necesita effect_FakeLoadingBar
    "SimulateMouseInvert": { "weight": 2, "cooldownKey": "majorCooldownClicks" }, // Necesita effect_SimulateMouseInvert
    "MASSIVEScoreBonus": { "weight": 3, "cooldownKey": "majorCooldownClicks", "params": {"amount": [100, 250]} }, // Necesita effect_MASSIVEScoreBonus
    "MASSIVEScorePenalty": { "weight": 3, "cooldownKey": "majorCooldownClicks", "params": {"amount": [50, 150]} }, // Necesita effect_MASSIVEScorePenalty
    "ULTIMATE_GLITCH": { "weight": 2, "cooldownKey": "majorCooldownClicks", "targets": ["titleEl", "doNotClickBtn", "messageBox", "alienImage", "scoreDisplay", "createSignBtn", "galleryBtn", "consoleOutput", "crazyTextEl"], "params": {"intensity": [10, 16]} }, // Necesita effect_ULTIMATE_GLITCH
    "FLY_EVERYTHING": { "weight": 2, "cooldownKey": "majorCooldownClicks", "targets": ["titleEl", "alienImage", "messageBox", "doNotClickBtn", "scoreDisplay", "createSignBtn", "galleryBtn", "consoleOutput"] }, // Necesita effect_FLY_EVERYTHING
    "BgImageFlicker": { "weight": 4, "cooldownKey": "majorCooldownClicks" } // Necesita effect_BgImageFlicker si backgroundImages in data.json
  }
}
